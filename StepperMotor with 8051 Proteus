#include <reg51.h>
#include <stdio.h>

void Delay(unsigned int k)
{
   unsigned int i, j;
   for (i=0;i<=k;i++)
      for(j=0;j<100;j++);
}

void main(void)
 { 
   // Write your code here
   P1=0x00;
   
   while (1){
      //
      P1=0x06;
      Delay(800);
      P1=0x03;
      Delay(800);
      P1=0x09;
      Delay(800);
      P1=0x0C;
      Delay(800);
      }
 }
 
 
