#include <reg51.h>
#include <stdio.h>
//sbit pin=P1^0;
void delay(int value )
{
int i,j;
 for(i=0;i<=value;i++)
 for(j=0;j<=5;j++);
}
void main()
{
   while(1){
      P1=0x01;  //So giving port 1 command 0x01(10000000) 					
      delay(500);
      P1=0x00;	//and 0x00(00000000) means all the pins are low.
      //pin=0;
      delay(500);
   }
}
